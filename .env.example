# Debug Mode Configuration
# Set to 'true' to enable debug mode, 'false' to disable
VITE_DEBUG_MODE_ENABLED=true

# Auto-detect debug mode based on environment
# When true: enables debug in development, disables in production
# When false: uses VITE_DEBUG_MODE_ENABLED value explicitly
VITE_DEBUG_MODE_AUTO=true

# Environment identifier (set automatically by Vite)
# DEV=true (development)
# PROD=true (production)